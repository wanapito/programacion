
public class capitulo20_sentencia_control_if {

	public static void main(String[] args) {
		// TODO Esbozo de método generado automáticamente
		var condicion = true;

//va implicito el ==
		if (condicion) {
			System.out.println("condcion  verdadera");
		}

		else {
			System.out.println("condicon falsa");
		}
	}

}
